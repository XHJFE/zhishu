    <title>地铁房价</title>
    <link rel="stylesheet" href="<%= root[env] %>css/house.css">
    <% include include/cssjs %>
</head>
<body>
<% include include/header %>
<% include include/anchor %>
<!--小区信息-->
<div class="top">
